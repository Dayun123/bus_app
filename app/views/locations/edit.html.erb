<div class="container">
  <div class="row">
    <div class="col-md-6 col-md-offset-3">
      <h1>Editing Location</h1>

      <% if notice %>
        <p id="notice"><%= notice %></p>
      <% end -%>

      <%= render 'form', location: @location %>

      <%= link_to 'Show', @location %> |
      <%= link_to 'Back', locations_path %>
    </div>
  </div>
</div>
